<%= form_with(model: record) do |form| %>
    <%= form.label :content, style: "display: block"  %>
    <%= form.text_field :content, style: "display: block"  %>

    <%= form.label :user_id, style: "display: block"  %>
    <%= form.number_field :user_id, style: "display: block"  %>

    <%= form.submit %>
<% end %>