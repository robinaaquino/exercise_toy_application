<h1> Records <h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Content</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @records.each do |record| %>
            <tr>
                <td><%= record.id%></td>
                <td><%= record.content%></td>
                <td></td>
                <td></td>
                <td></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>