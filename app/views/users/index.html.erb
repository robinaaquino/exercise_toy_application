<h1> Users <h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th colspan="3"></th>
        </tr>
    </thead>

    <tbody>
        <% @users.each do |user| %>
            <tr>
                <td><%= user.id%></td>
                <td><%= user.name%></td>
                <td><%= user.email%></td>
                <td></td>
                <td></td>
                <td><%= link_to "User details", user %></td>
            </tr>
        <% end %>
    </tbody>
</table>

<br>

<%= link_to "New user", new_user_path %>